#!/bin/bash
nohup python3 main.py -v 0 -gen 300 -pop 100 -depth 10 -clf knn -param 5 -execs 1 5 -dataset 2 -balanced 0 -path T10/ -fileID T10 -train_type 1 -optmize 2 1 TP 2 f1_S &
htop
~
nohup python3 main.py -v 0 -gen 300 -pop 100 -depth 10 -clf knn -param 5 -execs 6 10 -dataset 2 -balanced 0 -path T10/ -fileID T10 -train_type 1 -optmize 2 1 TP 2 f1_S &
htop
~
nohup python3 main.py -v 0 -gen 300 -pop 100 -depth 10 -clf knn -param 5 -execs 1 5 -dataset 2 -balanced 1 -path T10b/ -fileID T10b -train_type 0 -optmize 2 1 TP 2 f1_S &
htop
~
nohup python3 main.py -v 0 -gen 300 -pop 100 -depth 10 -clf knn -param 5 -execs 6 10 -dataset 2 -balanced 1 -path T10b/ -fileID T10b -train_type 0 -optmize 2 1 TP 2 f1_S &
htop
~

